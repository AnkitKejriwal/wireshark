#.FN_BODY Code/local  VAL_PTR = &opcode

  if (is_ExtensionField){
	hf_index = hf_camel_extension_code_local;
  }else if (camel_opcode_type == CAMEL_OPCODE_RETURN_ERROR){
	hf_index = hf_camel_error_code_local;  
  }		
  %(DEFAULT_BODY)s
